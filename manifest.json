{
  "manifest_version": 3,
  "name": "LifeHub",
  "version": "1.0",
  "action": {
    "default_title": "Click to open panel"
  },
  "side_panel": {
    "default_path": "sidepanel.html"
  },
  "content_scripts": [
    {
      "js": ["content-script.js"],
      "matches": ["https://www.google.com/*"]
    }
  ],
  "permissions": ["side_panel", "contextMenus"],
  "background": {
    "service_worker": "service_worker.js"
  }
}
